<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Glass Lewis UI" preserve-order="true" parallel="false" verbose="1">
    <listeners>
        <listener
                class-name="com.glasslewis.ui.retryfailedtestcases.RetryListenerClass"/>
         </listeners>

    <test name="Chrome">
        <parameter name="BrowserType" value="CHROME"/>
        <classes>
            <class name="com.glasslewis.ui.testcases.WDLandingPageTest"></class>
            <class name="com.glasslewis.ui.testcases.VoteCardPageTest"></class>
        </classes>
    </test>
</suite>
